{
    "tooling_environment": {
        "name": "Tooling Account [<<env>>]",
        "account_id": "<<tooling_account_id>>",
        "region": "us-east-1",
        "metrics_collection_interval_min": 10,
        "digest_report_period_hours" : 48, 
        "digest_cron_expression": "cron(5 8 * * ? *)",
        "grafana_instance": {
            "grafana_vpc_id": "vpc-123",
            "grafana_security_group_id": "sg-123"
        }
    },
    "monitored_environments": [
        {
            "name": "monitored1 [<<env>>]",
            "account_id": "1234567890",
            "region": "eu-central-1",
            "metrics_extractor_role_arn": "arn:aws:iam::123456789:role/role-salmon-cross-account-extract-metrics-dev"
        },
        {
            "name": "monitored2 [<<env>>]",
            "account_id": "0987654321",
            "region": "eu-central-1"
        },
        {
            "name": "monitored3 [<<env>>]",
            "account_id": "1234567890",
            "region": "us-west-2"
        }

    ],
    "delivery_methods": [
        {
            "name": "aws_ses",
            "delivery_method_type": "AWS_SES",
            "sender_email" : "<<sender_email>>"
        },
        {
            "name": "local_smtp",
            "delivery_method_type": "SMTP",
            "sender_email": "<<sender_email>>",
            "credentials_secret_name": "sm-my-smtp-server-creds"
        },
        {
            "name": "soname_slack",
            "delivery_method_type": "SLACK",
            "credentials_secret_name": "sm-soname-slack-creds"
        }
    ]
}